<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="gr.tei.erasmus.pp.eventmate">
	
	<application
		android:name="gr.tei.erasmus.pp.eventmate.app.App"
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name="gr.tei.erasmus.pp.eventmate.ui.mainActivity.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="gr.tei.erasmus.pp.eventmate.ui.login.LoginActivity" />
		<activity android:name="gr.tei.erasmus.pp.eventmate.ui.signup.SignupActivity" />
		<activity android:name="gr.tei.erasmus.pp.eventmate.ui.events.NewEventActivity"></activity>
	</application>

</manifest>